# extra: altgr+[ -> :+shift
bracketleft    5 5 1
# @-key
at             5 6 0
# extra: ue -> @
udiaeresis     5 6 8
# extra: UE -> @+shift
Udiaeresis     5 6 1
# comma
comma          5 3 8
less           5 3 1

#####################
# |Bit 6|POUND|  *  | ; ] | / ? | S_R |  =  | A_UP| HOME|

# pound
bar            6 0 8
brokenbar      6 0 8
acute          6 0 8
grave          6 0 8
# *
asterisk       6 1 16
# semicolon
semicolon      6 2 16
# extra: ae -> ;
adiaeresis     6 2 8
# extra: AE -> ;+shift
Adiaeresis     6 2 1
# extra: altgr+] -> ;+shift
bracketright   6 2 1
# clr/home
Home           6 7 8
# extra: altgr+\ -> clr/home
backslash      6 7 8
# right shift
# key is right shift
Shift_R        6 4 4
# =
equal          6 5 16
# arrow up / pi
asciicircum    6 6 8
degree         6 6 8
# extra: altgr+m -> arrow up+shift (pi)
mu             6 6 1
# / ?
slash          6 3 16
question       6 3 8

#####################
# |Bit 0| 1 ! |A_LFT| CTRL| R/S |SPACE|  C= |  Q  | 2 " |

# shift+1
exclam         0 0 8
# left arrow
End            0 1 8
# extra: underscore -> arrow left
#underscore     0 1 8
# extra: underscore -> cbm+@
# key requires SHIFT on host, generates a key without shift but with CBM on emulated machine
#underscore     5 6 2048+16
underscore     5 6 2064
# extra: altgr+~ -> arrow left
asciitilde     0 1 8
# extra: ssharp -> arrow left
ssharp         0 1 8
# ctrl
#Control_L      0 2 8+16384  key is CTRL
Control_L      0 2 16392
# shift+2
quotedbl       0 7 8
# space
space          0 4 8
# cbm
#Tab            0 5 8192+8  key is CBM
Tab            0 5 8200
#ISO_Left_Tab   0 5 8192+8  key is CBM
ISO_Left_Tab   0 5 8200
# run/stop
Escape         0 3 8

