      valid_ffmpeg_platform="yes"
    fi
  else
    valid_ffmpeg_platform="yes"
  fi

  if test x"$valid_ffmpeg_platform" = "xyes"; then
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: via pkg-config" >&5
printf "%s\n" "via pkg-config" >&6; }

