SVN=$ac_cv_prog_SVN
if test -n "$SVN"; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $SVN" >&5
printf "%s\n" "$SVN" >&6; }
else
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
