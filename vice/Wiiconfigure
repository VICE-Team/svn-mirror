ac_compiler_gnu=$ac_cv_cxx_compiler_gnu

    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking if the C++ compiler supports $TEST_FLAG" >&5
printf %s "checking if the C++ compiler supports $TEST_FLAG... " >&6; }

