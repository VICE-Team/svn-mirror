    if test x"$with_libieee1284" = "xyes"; then
      LIBIEEE1284_HEADERS_PRESENT=yes

                   for ac_header in ieee1284.h
do :
  ac_fn_c_check_header_compile "$LINENO" "ieee1284.h" "ac_cv_header_ieee1284_h" "$ac_includes_default"
