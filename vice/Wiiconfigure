	GLIB_CFLAGS=$pkg_cv_GLIB_CFLAGS
	GLIB_LIBS=$pkg_cv_GLIB_LIBS
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
	have_glib2_module=yes
