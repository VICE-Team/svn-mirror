    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

int
main (void)
{
int test;
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"
then :
   { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
                     ENABLE_ARCH=yes
                     march="-march=native"
else case e in #(
  e)  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
                     ENABLE_ARCH=no
                     march=""  ;;
esac
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam conftest.$ac_ext
    VICE_CFLAGS="$ORIG_CFLAGS"
  else
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking if the compiler supports -march=$enable_arch" >&5
printf %s "checking if the compiler supports -march=$enable_arch... " >&6; }
    ORIG_CFLAGS="$VICE_CFLAGS"
    VICE_CFLAGS="$VICE_CFLAGS -march=$enable_arch"
    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

int
main (void)
{
int test;
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_compile "$LINENO"
then :
   { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
                     ENABLE_ARCH=yes
                     march="-march=$enable_arch"
else case e in #(
  e)  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
                     ENABLE_ARCH=no
                     march=""  ;;
esac
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam conftest.$ac_ext
    VICE_CFLAGS="$ORIG_CFLAGS"
  fi
fi



