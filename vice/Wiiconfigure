GLIB_GENMARSHAL=$ac_cv_prog_GLIB_GENMARSHAL
if test -n "$GLIB_GENMARSHAL"; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $GLIB_GENMARSHAL" >&5
printf "%s\n" "$GLIB_GENMARSHAL" >&6; }
