if test ${ac_cv_path_UNZIPBIN+y}
then :
  printf %s "(cached) " >&6
else case e in #(
  e) case $UNZIPBIN in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_UNZIPBIN="$UNZIPBIN" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  case $as_dir in #(((
    '') as_dir=./ ;;
    */) ;;
