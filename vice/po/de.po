#: src/translate_text.c.po.c:3630
msgid "Use old NTSC sync factor"
msgstr "Alten NTSC Sync Faktor verwenden"

#: src/translate_text.c.po.c:3662
msgid "Set both CIA models (0 = old 6526, 1 = new 6526A)"
msgstr "Wähle beide CIA Modelle (0 = alt 6526, 1 = neu 6526A)"

#: src/translate_text.c.po.c:3678
